{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "ADMIN_USER": "@admin_user",
    "ADMIN_PASS": "@admin_pass",
    "PORT": "3000"
  }
}
